{
  "components/common/header": "<a href=\"#main-content\" class=\"skip-to-main-link\">{{lang 'header.skip_to_main'}}</a>\n\n\n<div class=\"aux-container\">\n  <div class=\"aux-components5\">\n    <span class=\"alert\" role=\"alert\">\n      <p><strong>Howdy! You are logged in to TechHub as a Guest.</strong><br>Guests can build quotes to send to approved purchasers, but only approved purchasers can complete an order in TechHub. Contact your local IT team with questions.</p>\n    </span>\n  </div>\n</div>\n\n\n<!-- CSS for guest banner at different window widths -->\n{{#unless customer.customer_group_id '==' 10}}\n    <style>\n        @media only screen and (min-width: 801px){\n            \n            .aux-container {\n                display: none;\n            }\n            \n            .header{\n                top:0px;\n            }\n            \n            body {\n                padding-top: 120px;\n            }\n        }\n        \n        @media only screen and (max-width: 800px){\n            \n            .aux-container {\n                display: none;\n            }\n            \n            .header{\n                top:0px;\n            }\n            \n            body {\n                padding-top: 90px;\n            }\n        }\n        \n    </style>\n{{/unless}}\n\n{{#if customer.customer_group_id '==' 10}}\n\n    <style>\n\n        @media only screen and (min-width: 588px){\n            .header{\n                top:62px;\n            }\n            \n            body {\n                padding-top: 180px;\n            }\n        }\n        \n        @media only screen and (min-width: 801px){\n            .header{\n                top:62px;\n            }\n            \n            body {\n                padding-top: 180px;\n            }\n        }\n        \n        @media only screen and (min-width: 1096px){\n            .header{\n                top:40px;\n            }\n            \n            body {\n                padding-top: 160px;\n            }\n        }\n        \n        @media only screen and (max-width: 587px){\n            .header{\n                top:84px;\n            }\n            \n            body {\n                padding-top: 180px;\n            }\n        }\n        \n    </style>\n{{/if}}\n<!-- Guest Banner CSS end -->\n\n\n{{#if banners.top}}\n<div class=\"banners\" data-banner-location=\"top\">\n    {{#each (limit banners.top_metadata 1)}}\n    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\n        <div data-event-type=\"promotion-click\">\n            {{{this.content}}}\n        </div>\n    </div>\n    {{/each}}\n</div>\n{{/if}}\n<header class=\"header\" role=\"banner\">\n    <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\n        <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\n    </a>\n    \n    <div class=\"header-content\">\n\n    {{#if page_type '===' 'default'}}\n        <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n            {{> components/common/store-logo}}\n        </h1>\n    {{else}}\n        <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n            {{> components/common/store-logo}}\n        </div>\n    {{/if}}\n    \n        \n        {{> components/common/quick-search}}\n\n        \n        {{> components/common/navigation}}\n    </div>\n    <!--Original nav/header code here-->\n    <div class=\"navPages-container\" id=\"menu\" data-menu>\n        {{> components/common/navigation-menu}}\n    </div>\n    {{{region name=\"header_navigation_bottom--global\"}}}\n</header>\n{{{region name=\"header_bottom--global\"}}}\n{{{region name=\"header_bottom\"}}}\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\" class=\"header-logo__link\" data-header-logo-link>\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n        <img class=\"header-logo-image-unknown-size\"\n             src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n             alt=\"{{settings.store_logo.title}}\"\n             title=\"{{settings.store_logo.title}}\">\n        {{else}}\n        <div class=\"header-logo-image-container\">\n            <img class=\"header-logo-image\"\n                 src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\n                 srcset=\"{{getImageSrcset1x2x settings.store_logo.image theme_settings.logo_size}}\"\n                 alt=\"{{settings.store_logo.title}}\"\n                 title=\"{{settings.store_logo.title}}\">\n        </div>\n        {{/if}}\n    {{else}}\n    <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n",
  "components/common/quick-search": "<div class=\"container\">\n    <form class=\"form\" action=\"{{urls.search}}\" method=\"get\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"header-search-input\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\"\n                       type=\"text\"\n                       name=\"search_query\"\n                       id=\"header-search-input\"\n                       placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\n                       autocomplete=\"off\"\n                       required\n                >\n                <button type=\"submit\" class=\"search-button\">Search</button>\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n</div>\n",
  "components/common/navigation": "<nav class=\"navUser\">\n    {{#or customer (unless settings.hide_price_from_guests)}}\n        {{> components/common/currency-selector}}\n    {{/or}}\n\n    {{#if theme_settings.show_channels}}\n        {{> components/common/channel-selector}}\n    {{/if}}\n\n    <ul class=\"navUser-section navUser-section--alt\">\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navUser-item\">\n                <a class=\"navUser-action navUser-action--storeCredit\"\n                   data-dropdown=\"storeCredit-dropdown\"\n                   data-options=\"align:bottom\"\n                   href=\"{{urls.cart}}\"\n                   aria-label=\"{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\"\n                >\n                    <span class=\"navUser-action-divider\">\n                        {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                    </span>\n                </a>\n                <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\n                    {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\n                </div>\n            </li>\n        {{/if}}\n        <li class=\"navUser-item\">\n            <a class=\"navUser-action navUser-item--compare\"\n               href=\"{{urls.compare}}\"\n               data-compare-nav\n               aria-label=\"{{lang 'common.compare'}}\"\n            >\n                {{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span>\n            </a>\n        </li>\n        {{#if theme_settings.social_icon_placement_top}}\n            <li class=\"navUser-item navUser-item--social\">\n                {{> components/common/social-links}}\n            </li>\n            <li class=\"navUser-item navUser-item--divider\">|</li>\n        {{/if}}\n        <li class=\"navUser-item\">\n            <button class=\"navUser-action navUser-action--quickSearch\"\n               type=\"button\"\n               id=\"quick-search-expand\"\n               data-search=\"quickSearch\"\n               aria-controls=\"quickSearch\"\n               aria-label=\"{{lang 'common.search'}}\"\n            >\n                {{lang 'common.search'}}\n            </button>\n        </li>\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navUser-item\">\n                <a class=\"navUser-action\"\n                   href=\"{{urls.gift_certificate.purchase}}\"\n                   aria-label=\"{{lang 'common.gift_cert'}}\"\n                >\n                    {{lang 'common.gift_cert'}}\n                </a>\n            </li>\n        {{/if}}\n        <li class=\"navUser-item navUser-item--account\">\n            <!--change account in nav based on group-->\n            {{#if customer}}\n                {{#if customer.customer_group_id '==' 10}}\n                    <a class=\"navUser-action\"\n                       href=\"{{urls.auth.logout}}\"\n                       aria-label=\"{{lang 'common.logout'}}\"\n                    >\n                        {{lang 'common.logout'}}\n                    </a>\n                {{else}}\n                <a class=\"navUser-action\"\n                   href=\"{{urls.account.index}}\"\n                   aria-label=\"{{lang 'common.account'}}\"\n                >\n                    {{lang 'common.account'}}\n                </a>\n                <a class=\"navUser-action\"\n                   href=\"{{urls.auth.logout}}\"\n                   aria-label=\"{{lang 'common.logout'}}\"\n                >\n                    {{lang 'common.logout'}}\n                </a>\n                {{/if}}\n\n            {{else}}\n                <a class=\"navUser-action\"\n                   href=\"https://tamu.xecurify.com/moas/broker/login/jwt/322830?client_id=avf77jyj03e4enfj81wyn17p5iouj63&redirect_uri=https://tamu.mybigcommerce.com/login/token/\"\n                   aria-label=\"{{lang 'common.login'}}\"\n                >\n                    {{lang 'common.login'}}\n                </a>\n                {{#if settings.account_creation_enabled}}\n                    <span class=\"navUser-or\" style=\"display: none;\">{{lang 'common.or'}}</span>\n                    <a class=\"navUser-action\"\n                       href=\"{{urls.auth.create_account}}\"\n                       aria-label=\"{{lang 'common.sign_up'}}\"\n                       style=\"display: none;\"\n                    >\n                        {{lang 'common.sign_up'}}\n                    </a>\n                {{/if}}\n            {{/if}}\n        </li>\n        <li class=\"navUser-item navUser-item--cart\">\n            <!--Change cart to quote based on group id-->\n            {{#if customer}}\n                {{#if customer.customer_group_id '==' 10}}\n                    <a class=\"navUser-action\"\n                       data-cart-preview\n                       data-dropdown=\"cart-preview-dropdown\"\n                       data-options=\"align:right\"\n                       href=\"{{urls.cart}}\"\n                       aria-label=\"{{lang 'cart.quote_nav_aria_label'}}\"\n                    >\n                        <span class=\"navUser-item-cartLabel\">{{lang 'common.quote'}}</span>\n                        <span class=\"countPill cart-quantity\"></span>\n                    </a>\n                    <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n                {{else}}\n            <a class=\"navUser-action\"\n               data-cart-preview\n               data-dropdown=\"cart-preview-dropdown\"\n               data-options=\"align:right\"\n               href=\"{{urls.cart}}\"\n               aria-label=\"{{lang 'cart.nav_aria_label'}}\"\n            >\n                <span class=\"navUser-item-cartLabel\">{{lang 'common.cart'}}</span>\n                <span class=\"countPill cart-quantity\"></span>\n            </a>\n                    <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n                {{/if}}\n            {{else}}\n                <a class=\"navUser-action\"\n                   data-cart-preview\n                   data-dropdown=\"cart-preview-dropdown\"\n                   data-options=\"align:right\"\n                   href=\"{{urls.cart}}\"\n                   aria-label=\"{{lang 'cart.quote_nav_aria_label'}}\"\n                >\n                    <span class=\"navUser-item-cartLabel\">{{lang 'common.quote'}}</span>\n                    <span class=\"countPill cart-quantity\"></span>\n                </a>\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n            {{/if}}\n            \n        </li>\n    </ul>\n    <div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" data-prevent-quick-search-close>\n        {{> components/common/quick-search name='nav-quick-search'}}\n    </div>\n</nav>\n",
  "components/common/navigation-menu": "<nav class=\"navPages\">\r\n    <div class=\"navPages-quickSearch\">\r\n        {{> components/common/quick-search name='nav-menu-quick-search'}}\r\n    </div>\r\n    \r\n    <!--\r\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\r\n        {{#each categories}}\r\n            <li class=\"navPages-item\">\r\n            {{#if ../theme_settings.navigation_design '===' 'simple'}}\r\n                {{> components/common/navigation-list}}\r\n            {{/if}}\r\n            {{#if ../theme_settings.navigation_design '===' 'alternate'}}\r\n                {{> components/common/navigation-list-alternate}}\r\n            {{/if}}\r\n            </li>\r\n        {{/each}}\r\n        {{#unless theme_settings.hide_content_navigation}}\r\n            {{#each pages}}\r\n                 <li class=\"navPages-item navPages-item-page\">\r\n                     <a class=\"navPages-action{{#if name '==' ../page.title}} activePage{{/if}}\"\r\n                        href=\"{{url}}\"\r\n                        aria-label=\"{{name}}\"\r\n                     >\r\n                         {{name}}\r\n                     </a>\r\n                 </li>\r\n             {{/each}}\r\n        {{/unless}}\r\n    </ul> -->\r\n    \r\n    <!-- Testing nav menu split -->\r\n    <div class=\"nav-container\">\r\n        <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\" id=\"left-nav\">\r\n            {{#each categories}}\r\n                <li class=\"navPages-item\">\r\n                {{#if ../theme_settings.navigation_design '===' 'simple'}}\r\n                    {{> components/common/navigation-list}}\r\n                {{/if}}\r\n                {{#if ../theme_settings.navigation_design '===' 'alternate'}}\r\n                    {{> components/common/navigation-list-alternate}}\r\n                {{/if}}\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n        \r\n        <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\" id=\"right-nav\">\r\n            \r\n            {{#unless theme_settings.hide_content_navigation}}\r\n                {{#each pages}}\r\n                     <li class=\"navPages-item navPages-item-page\">\r\n                         <!--<a class=\"navPages-action{{#if name '==' ../page.title}} activePage{{/if}}\"\r\n                            href=\"{{url}}\"\r\n                            aria-label=\"{{name}}\"\r\n                         >\r\n                             {{name}}\r\n                         </a>-->\r\n                         \r\n                         <!-- Testing for dropdown menu for non category pages-->\r\n                         {{> components/common/navigation-list}}\r\n                     </li>\r\n                 {{/each}}\r\n            {{/unless}}\r\n            \r\n    </ul>\r\n    </div>\r\n    \r\n    <ul class=\"navPages-list navPages-list--user\">\r\n        {{#if currency_selector.currencies.length '>' 1}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action has-subMenu\"\r\n                   href=\"#\"\r\n                   data-collapsible=\"navPages-currency\"\r\n                   aria-controls=\"navPages-currency\"\r\n                   aria-expanded=\"false\"\r\n                   aria-label=\"{{lang 'common.currency' code=currency_selector.active_currency_code}}\"\r\n                >\r\n                    {{lang 'common.currency' code=currency_selector.active_currency_code}}\r\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use href=\"#icon-chevron-down\"></use>\r\n                        </svg>\r\n                    </i>\r\n                </a>\r\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                    <ul class=\"navPage-subMenu-list\">\r\n                        {{#each currency_selector.currencies}}\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{{switch_url}}}\"\r\n                               aria-label=\"{{name}}\"\r\n                               data-currency-code=\"{{{code}}}\"\r\n                               {{#if cart_currency_switch_url.length '>' 0 }}\r\n                               data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\r\n                               {{/if}}\r\n                               data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\r\n                            >\r\n                                {{#if is_active}}\r\n                                    <strong>{{name}}</strong>\r\n                                {{else}}\r\n                                    {{name}}\r\n                                {{/if}}\r\n                            </a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                </div>\r\n            </li>\r\n        {{/if}}\r\n        {{#if theme_settings.show_channels}}\r\n            <li class=\"navPages-item\">\r\n                {{> components/common/channel-selector-mobile}}\r\n            </li>\r\n        {{/if}}\r\n        {{#if settings.gift_certificates_enabled}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\"\r\n                   href=\"{{urls.gift_certificate.purchase}}\"\r\n                   aria-label=\"{{lang 'common.gift_cert'}}\"\r\n                >\r\n                    {{lang 'common.gift_cert'}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if customer.store_credit.value '>' 0}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action navPages-action--storeCredit\">\r\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if theme_settings.product_list_display_mode '==' 'list'}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action navPages-action--compare\"\r\n                   href=\"{{urls.compare}}\"\r\n                   data-compare-nav\r\n                   aria-label=\"{{lang 'common.compare'}}\"\r\n                >\r\n                    {{lang 'common.compare'}}\r\n                    <span class=\"countPill countPill--positive countPill--alt\"></span>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if customer}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action has-subMenu\"\r\n                   href=\"{{urls.account.index}}\"\r\n                   data-collapsible=\"navPages-account\"\r\n                   aria-label=\"{{lang 'common.account'}}\"\r\n                >\r\n                    {{lang 'common.account'}}\r\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                        <svg><use href=\"#icon-chevron-down\" /></svg>\r\n                    </i>\r\n                </a>\r\n                <div class=\"navPage-subMenu\" id=\"navPages-account\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                    <ul class=\"navPage-subMenu-list\">\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.orders.all}}\"\r\n                               aria-label=\"{{lang 'account.nav.orders'}}\"\r\n                            >\r\n                                {{lang 'account.nav.orders'}}\r\n                            </a>\r\n                        </li>\r\n                        {{#if settings.returns_enabled}}\r\n                            <li class=\"navPage-subMenu-item\">\r\n                                <a class=\"navPage-subMenu-action navPages-action\"\r\n                                   href=\"{{urls.account.returns}}\"\r\n                                   aria-label=\"{{lang 'account.nav.returns'}}\"\r\n                                >\r\n                                    {{lang 'account.nav.returns'}}\r\n                                </a>\r\n                            </li>\r\n                        {{/if}}\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.inbox}}\"\r\n                               aria-label=\"{{lang 'account.mobile_nav.messages'}}\"\r\n                            >\r\n                                {{lang 'account.mobile_nav.messages'}}\r\n                            </a>\r\n                        </li>\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.addresses}}\"\r\n                               aria-label=\"{{lang 'account.nav.addresses'}}\"\r\n                            >\r\n                                {{lang 'account.nav.addresses'}}\r\n                            </a>\r\n                        </li>\r\n                        {{#if settings.show_payment_methods}}\r\n                            <li class=\"navPage-subMenu-item\">\r\n                                <a class=\"navPage-subMenu-action navPages-action\"\r\n                                   href=\"{{urls.account.payment_methods.all}}\"\r\n                                   aria-label=\"{{lang 'account.nav.payment_methods'}}\"\r\n                                >\r\n                                    {{lang 'account.nav.payment_methods'}}\r\n                                </a>\r\n                            </li>\r\n                        {{/if}}\r\n                        <!-- <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.wishlists.all}}\"\r\n                               aria-label=\"{{lang 'account.mobile_nav.wishlists'}}\"\r\n                            >\r\n                                {{lang 'account.mobile_nav.wishlists'}}\r\n                            </a>\r\n                        </li>\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.recent_items}}\"\r\n                               aria-label=\"{{lang 'account.nav.recently_viewed'}}\"\r\n                            >\r\n                                {{lang 'account.nav.recently_viewed'}}\r\n                            </a>\r\n                        </li>\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.details}}\"\r\n                               aria-label=\"{{lang 'account.nav.settings'}}\"\r\n                            >\r\n                                {{lang 'account.nav.settings'}}\r\n                            </a>\r\n                        </li> -->\r\n                    </ul>\r\n                </div>\r\n            </li>\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\"\r\n                   href=\"{{urls.auth.logout}}\"\r\n                   aria-label=\"{{lang 'common.logout'}}\"\r\n                >\r\n                    {{lang 'common.logout'}}\r\n                </a>\r\n            </li>\r\n        {{else}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\"\r\n                   href=\"{{urls.auth.login}}\"\r\n                   aria-label=\"{{lang 'common.login'}}\"\r\n                >\r\n                    {{lang 'common.login'}}\r\n                </a>\r\n                <!-- {{#if settings.account_creation_enabled}}\r\n                    {{lang 'common.or'}}\r\n                    <a class=\"navPages-action\"\r\n                       href=\"{{urls.auth.create_account}}\"\r\n                       aria-label=\"{{lang 'common.sign_up'}}\"\r\n                    >\r\n                        {{lang 'common.sign_up'}}\r\n                    </a>\r\n                {{/if}} -->\r\n            </li>\r\n        {{/if}}\r\n        {{#if theme_settings.social_icon_placement_top}}\r\n            <li class=\"navPages-item\">\r\n                {{> components/common/social-links}}\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n</nav>\r\n",
  "components/common/currency-selector": "{{#if currency_selector.currencies.length '>' 1}}\n<ul class=\"navUser-section\">\n    <li class=\"navUser-item\">\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\"\n           href=\"#\"\n           data-dropdown=\"currencySelection\"\n           aria-controls=\"currencySelection\"\n           aria-expanded=\"false\"\n        >\n            {{lang 'common.currency' code=currency_selector.active_currency_code}}\n            <i class=\"icon\" aria-hidden=\"true\">\n                <svg>\n                    <use href=\"#icon-chevron-down\" />\n                </svg>\n            </i>\n        </a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\"\n                   data-currency-code=\"{{{code}}}\"\n                   {{#if cart_currency_switch_url.length '>' 0 }}\n                   data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                   {{/if}}\n                   data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                >\n                {{#if is_active}}\n                    <strong>{{name}}</strong>\n                {{else}}\n                    {{name}}\n                {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </li>\n</ul>\n{{/if}}\n",
  "components/common/channel-selector": "{{#if theme_settings.channels.length '>' 1}}\n<ul class=\"navUser-section\">\n    <li class=\"navUser-item\">\n        <a href=\"#\"\n           class=\"navUser-action navUser-action--channelSelector has-dropdown\"\n           data-dropdown=\"channelSelection\"\n           aria-controls=\"channelSelection\"\n           aria-expanded=\"false\"\n        >\n            {{#each theme_settings.channels}}\n                {{#if is_active}}\n                    {{lang 'common.channel' code=code}}\n                {{/if}}\n            {{/each}}\n            <i class=\"icon\" aria-hidden=\"true\">\n                <svg>\n                    <use href=\"#icon-chevron-down\"/>\n                </svg>\n            </i>\n        </a>\n        <ul class=\"dropdown-menu\" id=\"channelSelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each theme_settings.channels}}\n                <li class=\"dropdown-menu-item\">\n                    <a href=\"{{{switch_url}}}\" data-channel-code=\"{{{code}}}\" data-warning=\"{{lang 'common.channel_switch_warning'}}\">\n                        {{#if is_active}}\n                            <strong>{{name}}</strong>\n                        {{else}}\n                            {{name}}\n                        {{/if}}\n                    </a>\n                </li>\n            {{/each}}\n        </ul>\n    </li>\n</ul>\n{{/if}}\n",
  "components/common/social-links": "{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            {{#if display_name '===' 'twitter'}}\n                <li class=\"socialLinks-item\">\n                    <a class=\"icon icon--x\"\n                    href=\"{{url}}\"\n                    target=\"_blank\"\n                    rel=\"noopener\"\n                    title=\"X\"\n                    >\n                        <span class=\"aria-description--hidden\">X</span>\n                        <svg>\n                            <use href=\"#icon-x\"/>\n                        </svg>\n                    </a>\n                </li>\n            {{else}}\n                <li class=\"socialLinks-item\">\n                    <a class=\"icon icon--{{display_name}}\"\n                    href=\"{{url}}\"\n                    target=\"_blank\"\n                    rel=\"noopener\"\n                    title=\"{{{capitalize display_name}}}\"\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\n                        <svg>\n                            <use href=\"#icon-{{display_name}}\"/>\n                        </svg>\n                    </a>\n                </li>\n            {{/if}}\n        {{/each}}\n    </ul>\n{{/if}}\n",
  "components/common/navigation-list": "{{#if children}}\n<a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   data-collapsible=\"navPages-{{id}}\"\n>\n    {{name}}\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n        <svg><use href=\"#icon-chevron-down\" /></svg>\n    </i>\n</a>\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item\">\n            <a class=\"navPage-subMenu-action navPages-action\"\n               href=\"{{url}}\"\n               aria-label=\"{{lang 'category.view_all.name' category=name}}\"\n            >\n                {{lang 'category.view_all.name' category=name}}\n            </a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n                       href=\"{{url}}\"\n                       aria-label=\"{{name}}\"\n                    >\n                        {{name}}\n                        <span class=\"collapsible-icon-wrapper\"\n                            data-collapsible=\"navPages-{{id}}\"\n                            data-collapsible-disabled-breakpoint=\"medium\"\n                            data-collapsible-disabled-state=\"open\"\n                            data-collapsible-enabled-state=\"closed\"\n                        >\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                                <svg><use href=\"#icon-chevron-down\" /></svg>\n                            </i>\n                        </span>\n                    </a>\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\n                        {{#each children}}\n                        <li class=\"navPage-childList-item\">\n                            <a class=\"navPage-childList-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                               href=\"{{url}}\"\n                               aria-label=\"{{name}}\"\n                            >\n                                {{name}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                       href=\"{{url}}\"\n                       aria-label=\"{{name}}\"\n                    >\n                        {{name}}\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n{{else}}\n<a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   aria-label=\"{{name}}\"\n>\n    {{name}}\n</a>\n{{/if}}\n",
  "components/common/navigation-list-alternate": "{{#if children}}\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       data-collapsible=\"navPages-{{id}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n        <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n            <svg><use href=\"#icon-chevron-down\" /></svg>\n        </i>\n    </a>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n       href=\"{{url}}\"\n       aria-label=\"{{name}}\"\n    >\n        {{name}}\n    </a>\n{{/if}}\n",
  "components/common/channel-selector-mobile": "{{#if theme_settings.channels.length '>' 1}}\n<a href=\"#\"\n   class=\"navPages-action has-subMenu\"\n   data-collapsible=\"navPages-channel\"\n   aria-controls=\"navPages-channel\"\n   aria-expanded=\"false\"\n   aria-label=\"\n    {{#each theme_settings.channels}}\n        {{#if is_active}}\n            {{lang 'common.channel' code=code}}\n        {{/if}}\n    {{/each}}\n   \"\n>\n    {{#each theme_settings.channels}}\n        {{#if is_active}}\n            {{lang 'common.channel' code=code}}\n        {{/if}}\n    {{/each}}\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n        <svg>\n            <use href=\"#icon-chevron-down\"></use>\n        </svg>\n    </i>\n</a>\n<div class=\"navPage-subMenu\" id=\"navPages-channel\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        {{#each theme_settings.channels}}\n        <li class=\"navPage-subMenu-item\">\n            <a href=\"{{{switch_url}}}\"\n               class=\"navPage-subMenu-action navPages-action\"\n               aria-label=\"{{name}}\"\n               data-channel-code=\"{{{code}}}\"\n               data-warning=\"{{lang 'common.channel_switch_warning'}}\"\n            >\n                {{name}}\n            </a>\n        </li>\n        {{/each}}\n    </ul>\n</div>\n{{/if}}\n",
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-parent\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n                        {{name}}<i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use href=\"#icon-chevron-down\" /></svg></i>\n                    </a>\n                    {{> components/common/navigation-dropdown}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n"
}
